{
  "version": "2016.0.0-beta1",
  "authors": [ "Dennis Apter" ],
  "description": "Common utilities library",
  "iconUrl": "https://raw.githubusercontent.com/dennis-apter/Exader/master/Exader.png",
  "projectUrl": "https://github.com/dennis-apter/Exader",
  "repository": {
    "type": "git",
    "url": "https://github.com/dennis-apter/Exader.git"
  },

  "compilationOptions": {
    "keyFile": "../../Exader.snk",
    "xmlDoc": true
  },

  "dependencies": {
    "JetBrains.Annotations": "10.1.4"
  },

  "frameworks": {
    "netstandard1.1": {
      "compilationOptions": {
        "define": [
          "NETCORE"
        ]
      },
      "dependencies": {
        "System.Linq": "4.0.1-*",
        "System.Runtime": "4.1.0-*",
        "System.Runtime.Extensions": "4.1.0-*",
        "System.ComponentModel": "4.0.1-*",
        "System.Globalization": "4.0.11-*",
        "System.Threading": "4.0.11-*",
        "System.Resources.ResourceManager": "4.0.1-*",
        "System.Security.SecureString": "4.0.0-*"
      },
      "imports": [
        "dotnet5.2"
      ]
    },
    "sl50": {
      "frameworkAssemblies": {
        "mscorlib": "5.0.5.0",
        "System": "5.0.5.0",
        "System.Core": "5.0.5.0"
      },
      "compilationOptions": {
        "define": [
          "SILVERLIGHT"
        ]
      }
    }
  }
}
